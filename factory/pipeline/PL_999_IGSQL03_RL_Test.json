{
	"name": "PL_999_IGSQL03_RL_Test",
	"properties": {
		"activities": [
			{
				"name": "Get DatabasesControl Table",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
