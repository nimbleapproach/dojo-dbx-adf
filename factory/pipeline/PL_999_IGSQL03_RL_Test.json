							"outputs": [
								{
									"referenceName": "RL_ADLS_CSV",
									"type": "DatasetReference",
									"parameters": {
										"pm_Environment": "dev",
										"pm_FileName": {
											"value": "@{replace(item().TABLE_NAME,' ','_')}",
											"type": "Expression"
										},
										"pm_FileLocation": {
											"value": "@{replace(item().TABLE_NAME,' ','_')}",
											"type": "Expression"
										},
										"pm_SourceFolderName": "test"
									}
								}
							]
						},
						{
