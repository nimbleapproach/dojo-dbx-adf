												"value": "@{replace(item().TABLE_NAME,' ','_')}",
												"type": "Expression"
											},
											"pm_Directory": "test"
										}
									}
								},
								"compute": {
									"coreCount": 8,
									"computeType": "General"
								},
								"traceLevel": "Fine"
							}
						}
					]
				}
			}
		],
		"parameters": {
			"filename": {
				"type": "string"
			}
		},
