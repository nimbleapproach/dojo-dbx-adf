									"referenceName": "RL_DF_TEST",
									"type": "DataFlowReference",
									"parameters": {
										"filename": {
											"value": "'@{concat(replace(item().TABLE_NAME,' ','_'), '.parquet')}'",
											"type": "Expression"
										}
									},
									"datasetParameters": {
										"LoadCSV": {
											"pm_Environment": "dev",
