					{
						"activity": "GetManifest",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"expression": {
						"value": "@contains(activity('GetManifest').output.firstRow,'subManifests')",
						"type": "Expression"
					},
					"ifTrueActivities": [
						{
							"name": "ForEachSubmanifest",
							"type": "ForEach",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"items": {
									"value": "@activity('GetManifest').output.firstRow.subManifests",
									"type": "Expression"
								},
								"isSequential": false,
								"activities": [
									{
										"name": "PL_15_NUVIAS_OPERATIONS_SUBITERATOR__1",
										"type": "ExecutePipeline",
										"dependsOn": [],
										"userProperties": [],
										"typeProperties": {
											"pipeline": {
												"referenceName": "PL_15_NUVIAS_OPERATIONS_SUBITERATOR__1",
												"type": "PipelineReference"
											},
											"waitOnCompletion": false,
											"parameters": {
												"pm_FullLoad": {
													"value": "@pipeline().parameters.pm_FullLoad",
													"type": "Expression"
												},
												"pm_ManifestPath": {
													"value": "@{pipeline().parameters.pm_ManifestPath}/@{item().manifestName}",
													"type": "Expression"
												},
												"pm_ManifestFile": {
													"value": "@item().manifestName",
													"type": "Expression"
												}
											}
										}
									}
								]
							}
						}
					]
				}
			},
			{
				"name": "IfEntitiesExists",
				"type": "IfCondition",
				"dependsOn": [
					{
						"activity": "GetManifest",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"expression": {
						"value": "@contains(activity('GetManifest').output.firstRow,'entities')",
						"type": "Expression"
					},
					"ifTrueActivities": [
						{
							"name": "ForEachEntities",
							"type": "ForEach",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"items": {
									"value": "@activity('GetManifest').output.firstRow.entities",
									"type": "Expression"
								},
								"isSequential": false,
								"activities": [
									{
										"name": "PL_20_NUVIAS_OPERATIONS_LOAD_ENTITY",
										"type": "ExecutePipeline",
										"dependsOn": [],
										"userProperties": [],
										"typeProperties": {
											"pipeline": {
												"referenceName": "PL_20_NUVIAS_OPERATIONS_LOAD_ENTITY",
												"type": "PipelineReference"
											},
											"waitOnCompletion": true,
											"parameters": {
												"pm_Entity": {
													"value": "@item().entityName",
													"type": "Expression"
												},
												"pm_ManifestPath": {
													"value": "@pipeline().parameters.pm_ManifestPath",
													"type": "Expression"
												},
												"pm_ManifestFile": {
													"value": "@pipeline().parameters.pm_ManifestFile",
													"type": "Expression"
												},
												"pm_FullLoad": {
													"value": "@pipeline().parameters.pm_FullLoad",
													"type": "Expression"
												}
											}
										}
									}
								]
							}
						}
					]
				}
			}
		],
		"parameters": {
			"pm_FullLoad": {
				"type": "bool",
				"defaultValue": false
			},
			"pm_ManifestPath": {
				"type": "string"
			},
			"pm_ManifestFile": {
				"type": "string"
			}
		},
		"folder": {
			"name": "10 Ingestion/NUVIAS/OPERATIONS"
		},
		"annotations": []
	}
}