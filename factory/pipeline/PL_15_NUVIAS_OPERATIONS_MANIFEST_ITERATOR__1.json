{
	"name": "PL_15_NUVIAS_OPERATIONS_MANIFEST_ITERATOR__1",
	"properties": {
		"activities": [
			{
				"name": "GetManifest",
				"type": "Lookup",
				"dependsOn": [
					{
						"activity": "CoppyManifestToADLS",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
