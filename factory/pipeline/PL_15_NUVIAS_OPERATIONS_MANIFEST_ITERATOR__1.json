				]
			}
		],
		"parameters": {
			"pm_FullLoad": {
				"type": "bool",
				"defaultValue": false
			},
			"pm_ManifestPath": {
				"type": "string"
			},
			"pm_ManifestFile": {
				"type": "string"
			}
		},
		"folder": {
			"name": "10 Bronze/NUVIAS/MANIFEST"
		},
		"annotations": []
	}
}