						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "PL_15_NUVIAS_OPERATIONS_SUBITERATOR__2",
							"type": "ExecutePipeline",
							"dependsOn": [],
							"policy": {
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "PL_15_NUVIAS_OPERATIONS_ITERATOR__2",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true,
								"parameters": {
									"pm_FullLoad": {
										"value": "@pipeline().parameters.pm_FullLoad",
										"type": "Expression"
									},
									"pm_ManifestPath": {
										"value": "@{pipeline().parameters.pm_ManifestPath}/@{item().manifestName}",
										"type": "Expression"
									},
									"pm_ManifestFile": {
										"value": "@item().manifestName",
										"type": "Expression"
									}
								}
							}
						}
					]
				}
			},
			{
				"name": "CoppyManifestToADLS",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "PreserveHierarchy"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DS_ADLS_JSON_NUVIAS_FO_MANIFEST",
						"type": "DatasetReference",
						"parameters": {
							"pm_Environment": {
								"value": "@pipeline().globalParameters.gpm_Environment",
								"type": "Expression"
							},
							"pm_ManifestPath": {
								"value": "@pipeline().parameters.pm_ManifestPath",
								"type": "Expression"
							},
							"pm_ManifestName": {
								"value": "@pipeline().parameters.pm_ManifestFile",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "DS_ADLS_JSON",
						"type": "DatasetReference",
						"parameters": {
